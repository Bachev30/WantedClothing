@model IEnumerable<Product>

<h2>Men's Collection</h2>
<div>
    @foreach (var product in Model)
    {
        <div>
            <img src="@product.ImageUrl" alt="@product.Name" />
            <h3>@product.Name</h3>
            <p>@product.Description</p>
            <p>Price: $@product.Price</p>
        </div>
    }
</div>